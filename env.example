# Symfony Application Environment
APP_ENV=dev
APP_SECRET=your-app-secret-key-here-change-this-in-production
APP_DEBUG=true

# Database Configuration
DB_DATABASE=symfony_app
DB_USERNAME=symfony_user
DB_PASSWORD=symfony_password
DATABASE_URL=postgresql://symfony_user:symfony_password@db:5432/symfony_app?serverVersion=16&charset=utf8

# Redis Configuration
REDIS_URL=redis://redis:6379

# JWT Authentication
JWT_SECRET_KEY=%kernel.project_dir%/config/jwt/private.pem
JWT_PUBLIC_KEY=%kernel.project_dir%/config/jwt/public.pem
JWT_PASSPHRASE=your-passphrase-here

# Mailer Configuration (optional)
# MAILER_DSN=smtp://localhost:1025

# CORS Configuration
CORS_ALLOW_ORIGIN='^https?://(localhost|127\.0\.0\.1)(:[0-9]+)?$'

# Rate Limiter (optional)
# RATE_LIMITER_LIMIT=100
# RATE_LIMITER_INTERVAL=60
